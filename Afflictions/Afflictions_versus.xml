      maxstrength="20"/>

    <Description
      text="This place is like a prison, I want to get out of here."
      target="Self"
      minstrength="20"
      maxstrength="50"/>

    <Description
      text="I hear the voice, it calls me!"
      target="Self"
      minstrength="50"
      maxstrength="75"/>
    
    <Description
      text="She awaits me, I must see her!"
      target="Self"
      minstrength="75"
      maxstrength="90"/>

    <Description
      text="LET ME OUT OF HERE!!!"
      target="Self"
      minstrength="90"
      maxstrength="100"/>

